#!/bin/sh


action "package install" "Installs ${package_name} ${package_version}."
action "package uninstall" "Installs ${package_name} ${package_version}."

action "configure" "Ensure that mysql.conf is in place"
action "initialize" "initdb for the mysql data directory"
action "start" "Start mysql server"
action "stop" "Stop mysql server"
action "restart" "Restart mysql server"

show_help usage

